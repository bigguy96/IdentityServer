@model IEnumerable<WeatherForecastViewModel>
@{
    ViewData["Title"] = "Home Page";
}

<h1>Home Page</h1>

<table class="table">
    <thead>
    <tr>
        <th>Date</th>
        <th>Temperature (Celsius)</th>
        <th>Temperature (Fahrenheit)</th>
        <th>Summary</th>
        <th></th>
    </tr>
    </thead>
    <tbody>
    @foreach (var m in Model)
    {
        <tr>
            <td>@m.Date</td>
            <td>@m.TemperatureC</td>
            <td>@m.TemperatureF</td>
            <td>@m.Summary</td>
            <td><a href="#">Edit</a> | <a href="#">Details</a> | <a href="#">Delete</a></td>
        </tr>
    }
    </tbody>
</table>